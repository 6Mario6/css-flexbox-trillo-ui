{
  "name": "trillo",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "npm-run-all --parallel serve watch:sass",
    "serve": "live-server --port=5173",
    "watch:sass": "sass --watch scss/main.scss css/style.css --style=expanded --source-map --embed-sources",
    "build:sass": "sass scss/main.scss css/style.css --style=compressed --no-source-map",
    "prefix:css": "postcss css/main.css -o css/main.css",
    "build": "npm-run-all build:sass prefix:css"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.94.0"
  }
}
